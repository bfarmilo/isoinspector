{
  "private": true,
  "name": "isoinspector",
  "version": "2.1.6",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest",
    "deploy_pi": "wsl rsync -azv --exclude-from='./.deploy_ignore' --delete ./build/ pi@192.168.50.59:/media/pi/main/Projects/isoinspector/"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "jest": {
    "moduleNameMapper": {
      "react": "preact-compat",
      "react-dom/test-utils": "preact/test-utils",
      "react-dom": "preact-compat",
      ".*css$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^2.2.0",
    "eslint": "^4.9.0",
    "eslint-config-synacor": "^2.0.2",
    "fs-extra": "^8.1.0",
    "identity-obj-proxy": "^3.0.0",
    "if-env": "^1.0.0",
    "jest": "^21.2.1",
    "preact-cli": "^2.2.1",
    "preact-cli-plugin-fast-async": "^1.0.1",
    "preact-render-spy": "^1.2.1"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.5.2",
    "codem-isoboxer": "^0.3.7",
    "mpeg2ts-parser": "^1.0.2",
    "pbf": "^3.2.1",
    "preact": "^10.2.1",
    "protobufjs": "^6.8.8"
  }
}
